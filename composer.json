{
  "name": "norbert/cloud_ldap_admin",
  "license": "proprietary",
  "type": "project",
  "autoload": {
    "psr-0": {
      "": "src/",
      "SymfonyStandard": "app/",
      "AppKernel": "app/"
    }
  },
  "require": {
    "php": "^7.0.8",
    "symfony/symfony": "~3.4",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/flex": "^1.0",
    "sensio/framework-extra-bundle": "^5.2",
    "symfony/monolog-bundle": "^3.2",
    "gregwar/captcha-bundle": "^2.0",
    "pear/crypt_gpg": "^1.6",
    "symfony/orm-pack": "^1.0",
    "symfony/webpack-encore-pack": "^1.0"
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.6",
    "symfony/web-server-bundle": "^3.4"
  },
  "scripts": {
    "auto-scripts": [
    ],
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
